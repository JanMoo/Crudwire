{
    "name": "JanMoo/Crudwire",
    "description": ":package_description",
    "license": "Mit",
    "authors": [
        {
            "name": "Jan Moorkens",
            "email": "janmoorkens@yahoo.com",
            "homepage": "https://jan.moorkens.mtantwerp.eu"
        }
    ],
    "homepage": "https://github.com/janmoo/crudwire",
    "keywords": ["Laravel", "Crudwire"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "livewire/livewire": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JanMoo\\Crudwire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JanMoo\\Crudwire\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JanMoo\\Crudwire\\CrudwireServiceProvider"
            ],
            "aliases": {
                "Crudwire": "JanMoo\\Crudwire\\Facades\\Crudwire"
            }
        }
    }
}
